#include <iostream>

using namespace std;

int main()
{
    int n=50,u;
    cin>>u;
    int arr[n+1];
    for(int i=2;i<=n;i++)
    {
        arr[i]=i;   
    }
    for(int i=2;i<u+1;i++)
    {
        int y=(i*i);
        if(arr[i]==i)
        {
      for(int j=y;j<u+1;j=j+i)
      {
        if(arr[j]==j)
        {
            arr[j]=i;
            
        }
        
        
      }  
        }   
        
    }
   while(u!=1) 
   {
        cout<<arr[u];
     u=u/arr[u];
    
   }
}
