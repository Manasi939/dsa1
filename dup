#include <bits/stdc++.h>

using namespace std;
void dup(string s,string nnn)
{
  if(s.length()==0)
  {
      return;
  }
   if(nnn.find(s[0])==-1)
   {
       
     cout<<s[0]  ;
     nnn.append(s,0,1);
     dup(s.substr(1),nnn);
   }
   else {
      dup(s.substr(1),nnn);  
   }
     
   
   
}
int main()
{
   string s;
   cin>>s;
   string nnn=" " ;
   dup(s,nnn);
}
