
#include <bits/stdc++.h>
#include<string>
#include<algorithm>
using namespace std;
void subset(int arr[],int n)
{ 
    int r=0;
   for(int i=0;i<64;i++)
   {  
       int sum=0;
       for(int j=0;j<n;j++)
   {
       if(arr[j] & 1<<i)
       {
           sum++;
       }
       
   }
     if(sum%3!=0) 
     {
         r= (r | 1<<i);
     }
   }
    
   cout<<r; 
}
int main()
{ 
    int arr[]={1,2,3,1,2,3,4,3,2,1};
    subset(arr,10);
    
}
