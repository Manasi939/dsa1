
#include <bits/stdc++.h>
#include<string>
#include<algorithm>
using namespace std;
void subset(int arr[],int n)
{ 
    int xorsum=0;
    for(int i=0;i<8;i++)
    {
      xorsum=xorsum^arr[i];
    }
    int i;
    for( i=0;!(xorsum & 1<<i);i++)
    {
    }
    int sum=0;
     for(int j=0;j<8;j++)
    {
       if(arr[j] & 1<<i)
       {
         sum=sum^arr[j]  ;
       }
        
    }
    cout<<sum;
    cout<<(sum^xorsum);
    
    
}
int main()
{ 
    int arr[]={1,2,3,4,6,3,2,1};
    subset(arr,7);
    
}
