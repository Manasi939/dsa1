#include <bits/stdc++.h>
#include<string>
#include<algorithm>
using namespace std;
int power(int n)
{
     return (n & n-1);
}
int main()
{
    int n,i;
    cin>>n;
    int k=0;
    while(n)
    {
        k++;
      n= power(n) ;  
     
    }
 cout<<k;
    
}
