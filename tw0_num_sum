
#include<bits/stdc++.h>
using namespace std;
void bruteforce(int arr[],int n,int k)
{
    for(int i=0;i<n-1;i++)  
    {
       for(int j=i+1;j<n;j++)  
    {
        if((arr[i]+arr[j])==k)
        {
            cout<<i<<j<<endl;
            break;
        }
            
    } 
    }
    
}
void myapp(int arr[],int n,int k)
{
    for(int i=0;i<n;i++)  
    {
        int f=k-arr[i];
         int j=i+1;
         int l=i+1,h=n-1;
         int mid=(h+l)/2;
         while(l<=h)
         {
             mid=((h+l)/2);
           if(arr[mid]==f)  
           {
               cout<<i<<mid<<endl;
               break;
           }
           else if(arr[mid]>f)
           {
              h=mid-1;
               
           }
           else{
               l=mid+1;
           }
           
         }
    }
    
}
void apnikaksh(int arr[],int n,int k)
{
    int l=0,h=n-1;
    int sum=0;
    while(l<h){
       sum=arr[l]+arr[h];
       if(sum==k) 
       {
          cout<<l<<h;
          break;
       }
       else if(sum<k)
       {
           l++;
       }
       else{
           h--;
       }
       
    }
} 
int main()
{
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    int k=0;
    cin>>k;
   // bruteforce(arr,n,k);
   //myapp(arr,n,k);
    apnikaksh(arr,n,k);
   
}

