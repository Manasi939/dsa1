
#include <bits/stdc++.h>
#include<string>
#include<algorithm>
using namespace std;
void subset(int arr[],int n)
{
    for(int i=0;i<(1<<3);i++)
    {
        for(int j=0;j<3;j++)
      {
           if(i & 1<<j)
           {
               cout<<arr[j]<<" ";
           }
        
        
      }
       cout<<endl;
    }
}
int main()
{ 
    int arr[]={1,2,3};
    subset(arr,3);
    
}
