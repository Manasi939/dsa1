#include <bits/stdc++.h>

using namespace std;
void toh(int n,int a,int b,int c)
{
    if(n==0)
    {
        return ;
    }
    toh(n-1,a,c,b) ;
    cout<<a<<b;
    toh(n-1,c,b,a);
}
int main()
{
    
    
    int n;
    cin>>n;
   toh(n,1,2,3);
}
