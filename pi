#include <bits/stdc++.h>

using namespace std;
void rev(string s,int n)
{
    if(s.length()==0)
    {
        return ;
    }
    int f;
    
    if(s[0]=='p' && s[1]=='i')
    {
       cout<<3.14;
       n=n+2;
       
       rev(s.substr(1), n);
       
    }
    else
    {cout<<s[0];
    
       n++;
        rev(s.substr(1),n);
    }
    
}
int main()
{
    string s;
    cin>>s;
     
    
    int n;
    rev(s,0);
}
