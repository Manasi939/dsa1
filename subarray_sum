#include <iostream>

using namespace std;
void sum(int i,int j,int arr[10])
{
    int s=0;
    for(int z=i;z<=j;z++)
    {
      s+=arr[z]  ;
    }
    cout<<s<<" ";
}
int main()
{
    int n;
    cin>>n;
    int arr[n];
    
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    for(int i=0;i<n;i++)
    {
          for(int j=i;j<n;j++)
    {
        sum(i,j,arr);
    }
    }
    
}
