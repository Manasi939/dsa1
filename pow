#include <iostream>

using namespace std;
int power(int n,int p)
{
    if(p==0)
    {
        return 1;
    }
    int prev=n*power(n,p-1);
    return prev;
}
int main()
{
    int n,p;
    cin>>n>>p;
    cout<<power(n,p);
   
    
}
