#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    int N=1e6,i;
    bool neg[N],pos[N];
    for( i=0;i<N;i++)
    {
        neg[i]=pos[i]=false;
    }
    for( i=0;i<n;i++)
    {
        if(arr[i]>=0)
        {
            pos[arr[i]]=true;
        }
             
    }
    for(i=0;i<N;i++)
    {
        if(!pos[i])
        {
           break; 
        }
    }
    
    cout<<i;
                    
    
   
}
