#include <bits/stdc++.h>
#include<string>
#include<algorithm>
using namespace std;
bool power(int n)
{
     return (n && (n & n-1));
}
int main()
{
    int n,i;
    cin>>n;
   if(!power(n)) cout<<"yes";
   else cout<<"no";
    
    
}
